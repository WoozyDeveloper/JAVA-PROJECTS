
public class Pacient extends Om {
  private String boala;

  public Pacient(String nume,String CNP,String boala) {
    super(nume,CNP);
    this.boala = boala;
  }

  public void setBoala(String boala) {
    this.boala = boala;
  }

  public String getBoala() {
    return boala;
  }

  public void showDescriere() {
    System.out.format("Pacientul %s (%s), ",this.nume,this.boala);
  }
}
